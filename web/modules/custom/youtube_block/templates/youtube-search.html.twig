<div class="class-overflow">
  <div id="youtube-block">
    {% for value in youtube_search %}
      <ul>
        <li>
          <span class="left-stat">
            <a href="//youtu.be/{{value['video_id']}}" target="_blank">
              <img src="{{ value['videos_img'] }}">
            </a>
          </span>
          <span>{{ value['title'] }}</span>
          <span class="youtube-stat">
            {{ value['duration'] }}
            {{ value['viewCount'] }}
            {{ value['likeCount'] }}
          </span>
        </li>
      </ul>
    {% endfor %}
  </div>
</div>


